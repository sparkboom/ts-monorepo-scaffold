{
  "name": "@%%APP_NAME%%/app",
  "version": "0.0.0",
  "description": "%%APP_TITLE%% - Front-End stand-alone application",
  "main": "src/index.tsx",
  "scripts": {
    "start": "webpack-dev-server --config webpack.conf.js --open",
    "start:dev": "webpack-dev-server --config webpack.conf.js --env.development --open",
    "start:prod": "webpack-dev-server --config webpack.conf.js --env.production --open",
    "build": "yarn clean && webpack --config webpack.conf.js",
    "build:dev": "yarn clean && webpack --config webpack.conf.js --env.development --verbose",
    "build:prod": "yarn clean && webpack --config ./webpack.conf.js --env.production",
    "build:analyze": "yarn clean && webpack --config ./webpack.conf.js --env.analyze",
    "clean": "shx rm -rf dist",
    "lint": "eslint src -c ../../.eslintrc.js --ext js,jsx,ts,tsx",
    "lint:fix": "eslint src -c ../../.eslintrc.js --ext js,jsx,ts,tx --fix",
    "checkTypes": "tsc",
    "test": "jest --config=../../jest.config.js --coverage",
    "test:watch": "jest --config=../../jest.config.js --watch"
  },
